# Database Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your-secure-password-here
POSTGRES_DB=ai_framework_test
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# LLM Configuration
AI_SDK_LLM_PROVIDER=openai
OPENAI_API_KEY=your-openai-api-key-here
GOOGLE_API_KEY=your-google-api-key-here
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Embedding Configuration (follows LLM provider)
AI_SDK_EMBEDDING_MODEL=text-embedding-3-small  # OpenAI: 1536 dims
# AI_SDK_EMBEDDING_MODEL=models/embedding-001  # Gemini: 768 dims  
# AI_SDK_EMBEDDING_MODEL=nomic-embed-text  # Ollama: 768 dims (local)

# Ollama (Local LLM)
OLLAMA_BASE_URL=http://localhost:11434

# Cloud Storage OAuth Configuration (Story 7.3)
# Google Drive OAuth 2.0 (Get credentials at: https://console.cloud.google.com/apis/credentials)
# 1. Create project in Google Cloud Console
# 2. Enable Google Drive API
# 3. Create OAuth 2.0 Client ID (Web application)
# 4. Add redirect URI: http://localhost:8000/api/oauth/callback
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=http://localhost:8000/api/oauth/callback

# Future: Microsoft Graph/SharePoint OAuth 2.0 (Story 7.4)
# Get credentials at: https://portal.azure.com/#blade/Microsoft_AAD_RegisteredApps
# MICROSOFT_CLIENT_ID=your-microsoft-client-id
# MICROSOFT_CLIENT_SECRET=your-microsoft-client-secret
# MICROSOFT_TENANT_ID=your-tenant-id (or 'common' for multi-tenant)
# MICROSOFT_REDIRECT_URI=http://localhost:8000/api/oauth/callback

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# Environment
ENVIRONMENT=development
